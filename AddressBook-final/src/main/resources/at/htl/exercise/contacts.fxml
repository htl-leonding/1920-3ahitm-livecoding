<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.htl.exercise.ContactsController">
   <children>
      <Label text="Contacts">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" editable="true">
        <columns>
          <TableColumn fx:id="firstNameColumn" prefWidth="202.0" text="First Name" />
          <TableColumn fx:id="lastNameColumn" prefWidth="181.0" text="Last Name" />
            <TableColumn fx:id="emailColumn" prefWidth="193.0" text="Email" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
         <children>
            <TextField fx:id="firstNameField" promptText="First Name" />
            <TextField fx:id="lastNameField" promptText="Last Name" />
            <TextField fx:id="emailField" promptText="Email" />
            <Button fx:id="addButton" disable="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="61.0" text="Add" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
